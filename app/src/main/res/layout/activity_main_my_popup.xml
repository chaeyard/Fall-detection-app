<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_my"
    android:orientation="vertical"
    android:background="@drawable/bg"
    >
    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:id="@+id/menu_4_1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="22.55dp"
            android:layout_marginTop="45dp"
            android:background="@drawable/menu"
            />
        <TextView
            android:layout_marginTop="20dp"
            android:layout_width="313dp"
            android:layout_height="20dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:fontFamily="@font/dnfbitbitttf"
            android:text="마이페이지"
            android:textColor="#FF7B29"
            android:textSize="20dp"
            android:textStyle="normal"
            />
        <RelativeLayout
            android:id="@+id/settings_6_"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="22.55dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="45dp"
            android:clipToOutline="true"
            android:background="@drawable/settings"
            />
    </LinearLayout>

    <FrameLayout
        android:orientation="horizontal"
        android:layout_width="365dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        >
        <View
            android:id="@+id/ellipse_5"
            android:layout_width="120dp"
            android:layout_height="120dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="62dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="53dp"
            android:background="@drawable/ellipse"
            />
        <View
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="163dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="93dp"
            android:background="@drawable/group"
            />
        <TextView
            android:layout_width="42dp"
            android:layout_height="17dp"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="212dp"
            android:layout_alignParentTop="true"
            android:layout_marginTop="81dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="17dp"
            android:text="박채원"
            android:gravity="top"
            />
        <TextView
            android:layout_width="40dp"
            android:layout_height="16dp"
            android:layout_marginLeft="212dp"
            android:layout_marginTop="104dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="16dp"
            android:text="사용자"
            android:gravity="top"
            android:alpha="0.6"
            />
        <TextView
            android:id="@+id/change_prof"
            android:layout_width="91dp"
            android:layout_height="15dp"
            android:layout_marginLeft="212dp"
            android:layout_marginTop="131dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#595085"
            android:textSize="13dp"
            android:text="Change profile"
            />
    </FrameLayout>

    <FrameLayout
        android:layout_width="365dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        >
        <TextView
            android:id="@+id/notice"
            android:layout_width="93dp"
            android:layout_height="26dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#99449B"
            android:textSize="26dp"
            android:text="NOTICE"
            android:gravity="top"
            />
        <View
            android:id="@+id/rectangle_1"
            android:layout_width="98dp"
            android:layout_height="90dp"
            android:layout_marginLeft="23dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/rectangle1"
            />
        <TextView
            android:layout_width="74dp"
            android:layout_height="12dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="74dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="12dp"
            android:text="낙상감지"
            />
        <TextView
            android:id="@+id/detect_fall_num"
            android:layout_width="15dp"
            android:layout_height="26dp"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="108dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="26dp"
            android:text="0"
            android:gravity="top"
            />
        <View
            android:id="@+id/rectangle_2"
            android:layout_width="98dp"
            android:layout_height="90dp"
            android:layout_marginLeft="131dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/rectangle2"
            />
        <TextView
            android:layout_width="74dp"
            android:layout_height="12dp"
            android:layout_marginLeft="143dp"
            android:layout_marginTop="74dp"
            android:text="긴급호출취소"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="12dp"
            />
        <TextView
            android:id="@+id/cancel_emergency_num"
            android:layout_width="14dp"
            android:layout_height="26dp"
            android:layout_marginLeft="143dp"
            android:layout_marginTop="108dp"
            android:text="0"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="26dp"
            />
        <View
            android:id="@+id/rectangle_3"
            android:layout_width="98dp"
            android:layout_height="90dp"
            android:layout_marginLeft="240dp"
            android:layout_marginTop="60dp"
            android:background="@drawable/rectangle3"
            />
        <TextView
            android:layout_width="74dp"
            android:layout_height="12dp"
            android:layout_marginLeft="252dp"
            android:layout_marginTop="74dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="12dp"
            android:text="긴급호출"

            android:gravity="top"
            />
        <TextView
            android:id="@+id/call_emergency_num"
            android:layout_width="14dp"
            android:layout_height="26dp"
            android:layout_marginLeft="252dp"
            android:layout_marginTop="108dp"
            android:fontFamily="@font/dnfbitbitttf"
            android:textColor="#000000"
            android:textSize="26dp"
            android:text="0"

            android:gravity="top"
            />

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="163dp"
            >
            <View
                android:id="@+id/bigrectangle_1"
                android:layout_width="153dp"
                android:layout_height="128dp"
                android:background="@drawable/bigrectangle1"
                />
            <View
                android:id="@+id/vector"
                android:layout_width="14dp"
                android:layout_height="18dp"
                android:layout_marginLeft="110dp"
                android:background="@drawable/vector"
                />
            <RelativeLayout
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:background="@drawable/user"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="40dp"
                />
            <TextView
                android:id="@+id/some_id"
                android:layout_width="77dp"
                android:layout_height="19dp"
                android:fontFamily="@font/dnfbitbitttf"
                android:textColor="#403572"
                android:textSize="19dp"
                android:text="보호자정보"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="65dp"
                />
        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="184dp"
            android:layout_marginTop="163dp">
            <View
                android:id="@+id/bigrectangle_2"
                android:layout_width="153dp"
                android:layout_height="128dp"
                android:background="@drawable/bigrectangle2"
                />
            <View
                android:id="@+id/vector2"
                android:layout_width="14dp"
                android:layout_height="18dp"
                android:layout_marginLeft="110dp"
                android:background="@drawable/vector2"
                />
            <RelativeLayout
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:background="@drawable/calendar"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="40dp"
                />
            <TextView
                android:layout_width="112dp"
                android:layout_height="19dp"
                android:fontFamily="@font/dnfbitbitttf"
                android:textColor="#479696"
                android:textSize="19dp"
                android:text="보호자 등록하기"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="65dp"
                />
        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="22dp"
            android:layout_marginTop="301dp">
            <View
                android:id="@+id/bigrectangle_3"
                android:layout_width="153dp"
                android:layout_height="128dp"
                android:background="@drawable/bigrectangle3"
                />
            <View
                android:id="@+id/vector3"
                android:layout_width="14dp"
                android:layout_height="18dp"
                android:layout_marginLeft="110dp"
                android:background="@drawable/vector3"
                />
            <RelativeLayout
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:background="@drawable/check_square"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="40dp"
                />
            <TextView
                android:layout_width="93dp"
                android:layout_height="19dp"
                android:fontFamily="@font/dnfbitbitttf"
                android:textColor="#FF5648"
                android:textSize="19dp"
                android:text="응급상세정보"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="65dp"
                />
        </FrameLayout>

        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="185dp"
            android:layout_marginTop="301dp">
            <View
                android:id="@+id/bigrectangle_4"
                android:layout_width="153dp"
                android:layout_height="128dp"
                android:background="@drawable/bigrectangle4"
                />
            <View
                android:id="@+id/vector4"
                android:layout_width="14dp"
                android:layout_height="18dp"
                android:layout_marginLeft="110dp"
                android:background="@drawable/vector4"
                />
            <RelativeLayout
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:background="@drawable/settings"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="40dp"
                />
            <TextView
                android:layout_width="32dp"
                android:layout_height="19dp"
                android:fontFamily="@font/dnfbitbitttf"
                android:textColor="#000000"
                android:textSize="19dp"
                android:text="설정"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="65dp"
                />
        </FrameLayout>

    </FrameLayout>

</LinearLayout>